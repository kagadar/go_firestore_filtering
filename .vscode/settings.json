{
  "files.exclude": {
    "bazel-*/": true,
  },
  "go.toolsManagement.autoUpdate": true,
  "go.goroot": "${workspaceFolder}/bazel-go_firestore_filtering/external/go_sdk",
  "go.toolsEnvVars": {
    "GOPACKAGESDRIVER": "${workspaceFolder}/tools/gopackagesdriver.sh"
  },
  "go.enableCodeLens": {
    "references": false,
    "runtest": false
  },
  "gopls": {
    "formatting.gofumpt": true,
    "formatting.local": "github.com/kagadar/go_firestore_filtering",
    "ui.completion.usePlaceholders": true,
    "ui.semanticTokens": true,
    "ui.codelenses": {
      "gc_details": false,
      "regenerate_cgo": false,
      "generate": false,
      "test": false,
      "tidy": false,
      "upgrade_dependency": false,
      "vendor": false
    },
  },
  "go.useLanguageServer": true,
  "go.buildOnSave": "off",
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
  "git.autoRepositoryDetection": false,
  "copyrightInserter.holder": "The Go Firestore Filtering Authors",
  "copyrightInserter.useLineComment": true,
}
